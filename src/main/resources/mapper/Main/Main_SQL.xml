<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.GW.JJOFFICE.JJOFFICE.main.repository.MainRepository">
    <select id="getTestSelect" resultType="map">
        select
            *
        from
            user
    </select>
    <select id="getTestOne" parameterType="map" resultType="map">
        select
            *
        from
            user
        where
            user_email = #{userEmail}
    </select>
</mapper>